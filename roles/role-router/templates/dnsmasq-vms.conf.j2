dhcp-host={{ (dhcp_host.name | split(','))[1] }},{{ hostvars[(dhcp_host.name | split(','))[0]].ansible_host }}.{{ domain }},{{ prefix_24 }}.{{ dhcp_host.value }}
{% endif %}
{% endfor %}
